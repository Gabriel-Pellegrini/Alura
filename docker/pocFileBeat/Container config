docker run -it --name logstash -h ubuntu1604 -v "C:\Users\gabri\Documents\Projetos\appdocker\volumes\Ubuntu:/var/dockerapp" -p 5044:5044 --network localNetwork gabrielpellegrini/linux /bin/bash

docker run -it gabrielpellegrini/filebeat:6.2.2

docker run --rm -it -v "$PWD/:/usr/share/logstash/pipeline/" docker.elastic.co/logstash/logstash:6.2.2



docker run --name kibana --link elasticsearch:elasticsearch -p 5601:5601 -e SERVER_NAME=kibana -e ELASTICSEARCH_URL=http://elasticsearch:9200 --network localNetwork docker.elastic.co/kibana/kibana:6.2.2

